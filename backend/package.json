    "@prisma/client": "5.17.0",
    "@types/cors": "^2.8.17",
    "@types/lodash": "^4.17.7",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-iso-date": "^3.6.1",
    "graphql-type-json": "^0.3.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
