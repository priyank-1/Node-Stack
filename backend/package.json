    "@types/express": "^4.17.21",
    "@types/graphql-iso-date": "^3.4.3",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.14.12",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "bullmq": "^5.12.1",
